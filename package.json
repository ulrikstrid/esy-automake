{
  "name": "esy-automake",
  "version": "0.0.000",
  "description": "automake-1.16 packaged for esy",
  "esy": {
    "buildsInSource": true,
    "build": [
      "find ./ -exec touch -t 200905010101 {} + ",
      ["bash", "-c", "#{os == 'windows' ? './configure --prefix=$cur__install --host x86_64-w64-mingw32' : './configure --prefix=$cur__install'}"],
      ["make"],
      ["make", "install"]
    ]
  },
  "dependencies": {
    "autoconf": "esy-packages/esy-autoconf#ea58671a08c7ebf89b0cebd41f4950ca67157b01"
  },
  "resolutions": {}
}
